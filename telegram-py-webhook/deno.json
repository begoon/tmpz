{
    "version": "0.4.6",
    "unstable": ["kv", "cron"],
    "tasks": {
        "tunnel": "deno run -A --watch=tunnel.ts,ngrok/ ./tunnel.ts",
        "webhook": "deno run -A ./webhook.ts",
        "webhook:info": "deno run -A ./webhook.ts --info",
        "webhook:delete": "deno run -A ./webhook.ts --delete"
    },
    "compilerOptions": {
        "allowJs": true,
        "strict": true
    },
    "lint": {
        "include": ["*.ts"],
        "exclude": ["./z/**"]
    },
    "imports": {
        "std/": "https://deno.land/std@0.211.0/"
    }
}
